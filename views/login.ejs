<div class="login-box">
    <h1>login</h1>
    <div class="form-box">
        <form action="/login" method="POST">
            <div class="form-in"><input id="email" type="email" name="email"required/><label class="label-name" for="email"><span class="content-dt">E-mail</span></label></div>
            <div class="form-in"><input id="password" type="password" name="password" required/><label class="label-name" for="password"><span class="content-dt">Password</span></label></div>
            <%- include('./partials/logErrorMsg') %>
            <button type="submit"> Login Now </button>
        </form>
        <div class="acc-pag">
        <P>If you don't have account You can create it from here !</P>
        <a class="btn" href="/register"> I don't have account </a>
        </div>
    </div>
</div>
